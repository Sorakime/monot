{
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:jsdoc/recommended"
  ],
  "plugins": ["node", "browser"],
  "parserOptions": {
    "node": true,
    "es2021": true,
    "ecmaVersion": "latest",
    "browser": true
  },
  "env": {
    "node": true,
    "es2021": true
  },
  "eslint.workingDirectories": [
    { "mode": "auto" }
  ],
  "globals": {}
}
