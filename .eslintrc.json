{
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:jsdoc/recommended"
  ],
  "plugins": ["node"],
  "parserOptions": {
    "node": true,
    "es2021": true,
    "ecmaVersion": "latest",
    "browser": true
  },
  "env": {
    "node": true,
    "es2021": true,
    "browser": true
  },
  "globals": {
    "node": false
  },
  "rules": {
    "node/no-unpublished-require": [0],
    "no-unused-vars": [0]
  }
}
